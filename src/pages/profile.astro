---
import Layout from '../layouts/Layout.astro';
import Hero from '@/components/Hero.astro';
import RecentProjects from '@/components/RecentProjects.astro';
import SidebySide from '@/components/SidebySide.astro';

// Import JSON data
import profileData from '@/data/profile.json';
---

<Layout
  title={`${profileData.profileBio.name} - ${profileData.profileBio.title}`}
  description={profileData.profileBio.summary}
  hasHero={true}
>
  <Hero />
  <SidebySide />
  <RecentProjects />
</Layout>
